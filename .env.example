# Ngrok configuration
NGROK_AUTH_TOKEN=""        # Your ngrok authentication token
NGROK_HTTPS_PORT="8080"    # Local port for HTTPS tunnel
NGROK_TCP_PORT="4000"      # Local port for TCP tunnel
NGROK_REGION="us"          # Ngrok region (us, eu, au, ap, sa, jp, in)

# Server identification
SERVER_NAME="my-vpn-server"  # Server name for certificates and configuration
ORGANIZATION="MyOrg"        # Organization name for certificates
ORG_UNIT="IT"               # Organizational unit for certificates

# OpenVPN configuration
OPENVPN_PORT="1194"        # Port for OpenVPN server
OPENVPN_PROTOCOL="udp"     # Protocol (udp or tcp)
OPENVPN_NETWORK="10.8.0.0" # VPN network address
OPENVPN_NETMASK="255.255.255.0" # VPN network mask
OPENVPN_DNS1="8.8.8.8"    # Primary DNS server
OPENVPN_DNS2="8.8.4.4"    # Secondary DNS server
